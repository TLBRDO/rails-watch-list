<div class="mt-3 border rounded">
  <div class="p-5">
    <h1>Lists</h1>
    <ul>
      <% @lists.each do |list| %>
        <li class="border-start border-5"><%= link_to list.name, list_path(list), class: "text-decoration-none ms-3" %></li>
        <% end %>
    </ul>
    <%= link_to new_list_path, class: "text-decoration-none" do %>
      <i class="fa-solid fa-plus"></i>
    <% end %>
  </div>
</div>
