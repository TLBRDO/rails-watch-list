<h1 class="mt-3 text-center">Add a new bookmark</h1>
<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.submit "Save comment", class: "btn btn-primary" %>
<% end %>
<div class="mt-3">
<%= link_to lists_path, class: "text-decoration-none" do %>
  <i class="fa-solid fa-house"></i>
<% end %>
</div>
